<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test_Suite_Emulators_Parallel_Real" parallel="tests" thread-count="3">

    <!-- systemPort and chromeDriverPort must be unique for Parallel execution -->
    <listeners>
        <listener class-name="com.appium.listeners.ListenerClass"></listener>
        <listener class-name="com.appium.listeners.AnnotationTransformer"></listener>
        <listener class-name="com.appium.listeners.MethodInterceptor"></listener>
    </listeners>

    <parameter name="runLocation" value="local"></parameter>

    <test name="Tests_on_iOS_14">
        <parameter name="simulator" value="false"></parameter>
        <parameter name="platformName" value="iOS"></parameter>
        <parameter name="platformVersion" value="14"></parameter>
        <parameter name="udid" value="00008020-000328CE14A3002E"></parameter>
        <parameter name="installApp" value="false"></parameter>
        <parameter name="deviceName" value="TestPhone-IOS"></parameter>
        <parameter name="systemPort" value="10031"/>
        <parameter name="chromeDriverPort" value="11032"/>
        <parameter name="wdaLocalPort" value="10003"/>
        <parameter name="webkitDebugProxyPort" value="10013"/>
        <parameter name="comPort" value=""/>
        <classes>
            <class name="com.appium.runners.iOS.iOS_Settings_Runner"></class>
        </classes>
    </test>

    <test name="Tests_on_Android_12">
        <parameter name="emulator" value="false"></parameter>
        <parameter name="platformName" value="Android"></parameter>
        <parameter name="platformVersion" value="12"></parameter>
        <parameter name="udid" value="192.168.165.76:5555"></parameter>
        <parameter name="installApp" value="true"></parameter>
        <parameter name="deviceName" value="TestPhone-ANDROID"></parameter>
        <parameter name="systemPort" value="10032"/>
        <parameter name="chromeDriverPort" value="11033"/>
        <parameter name="wdaLocalPort" value="1002"/>
        <parameter name="webkitDebugProxyPort" value="9228"/>
        <parameter name="userId" value="12001264"/>
        <parameter name="comPort" value=""/>

        <classes>
            <class name="com.appium.runners.Android.Andr12_RevE_Tap_Runner"></class>
        </classes>
    </test>

    <test name="Tests_on_Android_7">
        <parameter name="emulator" value="false"></parameter>
        <parameter name="platformName" value="Android"></parameter>
        <parameter name="platformVersion" value="7"></parameter>
        <parameter name="udid" value="03157df343476022"></parameter>
        <parameter name="installApp" value="true"></parameter>
        <parameter name="deviceName" value="TestPhone-ANDROID"></parameter>
        <parameter name="systemPort" value="10033"/>
        <parameter name="chromeDriverPort" value="11033"/>
        <parameter name="wdaLocalPort" value="1002"/>
        <parameter name="webkitDebugProxyPort" value="9228"/>
        <parameter name="userId" value="12001264"/>
        <parameter name="comPort" value=""/>

        <classes>
            <class name="com.appium.runners.Android.Andr7_SettingsScenario_Runner"></class>
        </classes>
    </test>

</suite>